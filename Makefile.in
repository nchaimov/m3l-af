intro:	
	@echo ""
	@echo ""
	@echo ":: Building PhyML version $(VERSION) ::"
	@echo ""
	@echo ""

putversion:
	sed -e 's/#define VERSION.*/#define VERSION \"$(VERSION)\"/' utilities.h > new_utilities.h
	mv new_utilities.h utilities.h

phyml$(EXEEXT): intro putversion $(phyml_LDADD) $(phyml_SOURCES)
	$(SLINK) $(phyml_LDADD) -o $@
	@echo ""
	@echo "Done."

